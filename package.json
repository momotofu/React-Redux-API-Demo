{
  "name": "ReactRedux-demo",
  "version": "1.0.0",
  "scripts": {
    "test": "NODE_ENV=test jest --coverage",
    "watch:test": "jest --watch",
    "build": "rm dist/*; NODE_ENV=production webpack",
    "prod": "node server.js",
    "dev": "NODE_ENV=dev webpack-dev-server",
    "start": "NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/momotofu/React-Redux-Demo"
  },
  "author": "Christopher Reece",
  "license": "ISC",
  "engines": {
    "node": ">=0.12"
  },
  "engineStrict": true,
  "dependencies": {
    "ajv": "^6.5.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^15.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^2.5.1",
    "enzyme-to-json": "^1.3.0",
    "file-loader": "^2.0.0",
    "jest": "^16.0.2",
    "jest-cli": "15.1.0",
    "react-addons-test-utils": "^15.3.2",
    "react-scripts": "^2.0.1",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-url-loader": "^1.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.9",
    "es6-promise": "^4.0.5",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.3",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^12.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/components/history-accordion/*.js",
      "src/actions/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    }
  }
}
